            var id = tr.find("#lblsnonew2").text();
            var isSuccess = -1;
            var model =
            {
                "_fccc": fc,
                "_asset_id": id
            };
            $.ajax({
                url: '@Url.Action("SaveData","AssetTransfer")',
                data: JSON.stringify(model),
                type: 'POST',
