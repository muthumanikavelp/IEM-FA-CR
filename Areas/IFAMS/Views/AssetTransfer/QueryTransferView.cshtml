    function valueChange(obj) {
        var ReturnDate = $.trim(obj.val());
        if (ReturnDate != "" && parseFloat(ReturnDate) != 0) {
            obj.addClass("valid");
            obj.removeClass("required");
        }
        else {
            obj.addClass("required");
            obj.removeClass("valid");
        }
    }

